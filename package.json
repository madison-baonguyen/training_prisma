{
  "name": "typescript-graphql",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "dev-apollo": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "seed": "prisma db seed --preview-feature",
    "seed-user": "ts-node ./src/seed-users.ts",
    "test": "TEST=true jest",
    "db:push": "prisma db push --preview-feature",
    "mysql:start": "docker-compose up -d",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/hapi": "^19.2.0",
    "@hapi/joi": "^17.1.1",
    "@prisma/client": "^2.20.0",
    "@sendgrid/mail": "^7.4.2",
    "apollo-server": "2.22.2",
    "date-fns": "^2.20.0",
    "dotenv": "^8.2.0",
    "graphql": "15.5.0",
    "graphql-iso-date": "3.6.1",
    "graphql-type-json": "^0.3.2",
    "hapi-auth-jwt2": "^10.1.0",
    "hapi-pino": "^8.3.0",
    "jest": "^26.6.3",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "prisma": "2.20.1",
    "ts-jest": "^26.5.4"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/graphql-iso-date": "3.4.0",
    "@types/hapi__hapi": "^19.0.4",
    "@types/hapi__joi": "^17.1.4",
    "@types/hapi-pino": "^8.0.1",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "12.20.7",
    "@types/ws": "7.4.1",
    "prisma": "2.20.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
